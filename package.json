{
  "name": "artisan-competent",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:staging": "ng serve -c staging",
    "dev": "ng serve --hmr",
    "watch": "ng build --watch --configuration development",
    "build": "ng build",
    "build:staging": "ng build -c staging",
    "build:prod": "ng build -c production",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "singleAttributePerLine": false,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular",
          "tabWidth": 4
        }
      }
    ],
    "endOfLine": "lf"
  },
  "private": true,
  "engines": {
    "node": ">=18.18 <21"
  },
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/montserrat": "^5.2.8",
    "@fontsource/poppins": "^5.2.7",
    "@fontsource/roboto": "^5.2.9",
    "@jsverse/transloco": "^8.2.0",
    "@ng-icons/core": "^32.5.0",
    "@ng-icons/heroicons": "^32.5.0",
    "@ng-icons/lucide": "^32.5.0",
    "iconify-icon": "^3.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.22",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2"
  }
}
